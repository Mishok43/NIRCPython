{
	"loss": {
		"otype": "RelativeL2"
	},
	"optimizer": {
		"otype": "Adam",
		"learning_rate": 1e-2,
		"beta1": 0.9,
		"beta2": 0.99,
		"epsilon": 1e-15,
		"l2_reg": 1e-6
	},
	"encoding": {
        "otype": "Composite",
        "nested": [
            {
                "n_dims_to_encode": 2,
                "otype": "OneBlob",
                "n_bins": 4
            },
            {
                "n_dims_to_encode": 2,
                "otype": "DenseGrid",
                "n_levels": 38,
                "base_resolution": 400,
                "interpolation": "Nearest",
                "per_level_scale": 1
            },
            {
                "n_dims_to_encode": 1,
                "otype": "OneBlob",
                "n_bins": 4
            },
            {
                "n_dims_to_encode": 3,
                "otype": "Identity"
            },
            {
                "n_dims_to_encode": 3,
                "otype": "SphericalHarmonics",
                "degree": 5
            }
        ]

	},
	"network": {
		"otype": "FullyFusedMLP",
		"activation": "ReLU",
		"output_activation": "ReLU",
		"n_neurons": 64,
		"n_hidden_layers": 6
	}
}
